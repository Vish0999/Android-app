<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5"
    android:background="@drawable/man">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="285dp"
        android:layout_height="63dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="63dp"
        android:elevation="70dp"
        app:cardCornerRadius="30dp"
        app:cardElevation="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white">

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="111dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="121dp"
                android:text="BooKing"
                android:textColor="@color/black"
                android:textSize="25sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <EditText
        android:id="@+id/email1"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="19dp"
        android:background="@drawable/bg"
        android:drawableLeft="@drawable/baseline_email_24"
        android:drawablePadding="7dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName"
        android:paddingLeft="25dp"
        android:paddingTop="5dp"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/uname" />

    <EditText
        android:id="@+id/uname"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="17dp"
        android:background="@drawable/bg"
        android:drawableLeft="@drawable/baseline_person_24"
        android:drawablePadding="7dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:paddingLeft="25dp"
        android:paddingTop="5dp"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cid" />

    <EditText
        android:id="@+id/dist"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/bg"
        android:drawableLeft="@drawable/baseline_social_distance_24"
        android:drawablePadding="7dp"
        android:ems="10"
        android:hint="Distination"
        android:inputType="textPersonName"
        android:paddingLeft="25dp"
        android:paddingTop="5dp"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <EditText
        android:id="@+id/secdist"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/bg"
        android:drawableLeft="@drawable/baseline_social_distance_24"
        android:drawablePadding="7dp"
        android:ems="10"
        android:hint="second Distination"
        android:inputType="textPersonName"
        android:paddingLeft="25dp"
        android:paddingTop="5dp"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="14dp"
        android:text="Distaination"
        android:textColor="@color/white"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email1" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="16dp"
        android:text="To"
        android:textColor="@color/white"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dist" />

    <Button
        android:id="@+id/btnbook"
        android:layout_width="146dp"
        android:layout_height="49dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="133dp"
        android:background="@color/white"
        android:text="Book"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secdist" />

    <EditText
        android:id="@+id/id"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="23dp"
        android:background="@drawable/bg"
        android:drawableLeft="@drawable/baseline_person_24"
        android:ems="10"
        android:hint="Enter Id"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColorHint="@color/white"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView6" />

    <EditText
        android:id="@+id/cid"
        android:layout_width="301dp"
        android:layout_height="48dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/bg"
        android:drawableLeft="@drawable/baseline_person_24"
        android:ems="10"
        android:hint="confirm Id"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:textColorHint="@color/white"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id" />

</androidx.constraintlayout.widget.ConstraintLayout>